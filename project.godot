config_version=4

[application]

config/name="Himari_vs_Monika"
config/description="Just Himari and Monika!"
run/main_scene="res://Scenes/MainMenu.tscn"
boot_splash/image="res://Splash.png"
boot_splash/bg_color=Color( 1, 1, 1, 1 )
config/icon="res://icon.png"
config/windows_native_icon="res://icon.ico"

[autoload]

Global="*res://Scripts/Global.gd"
SoundHandler="*res://Scenes/SoundHandler.tscn"
FpsCounter="*res://Scenes/FPSCounter.tscn"
ParticlesCache="*res://Scenes/ParticlesCache.tscn"
LevelEnd="*res://Scenes/LevelEnd.tscn"

[display]

window/size/width=1040
window/size/height=585
window/size/resizable=false
window/handheld/orientation="sensor_landscape"
window/stretch/mode="2d"
window/stretch/aspect="keep"

[gui]

theme/custom="res://Themes/Theme.tres"
theme/custom_font="res://Fonts/DynamicFont.tres"

[importer_defaults]

ogg_vorbis={
"loop": false
}

